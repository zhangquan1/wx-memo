.page{
    position:relative;
}
.list{
    position:absolute;
    width:100%;
    height:100%;
    top:0;
    left: 0;
}
.list-i{
    height:200rpx;
    width:100%;
    background: #eee;
    display: flex;
    flex-direction: column;
    margin:10px 0;
}
.content{
    height:110rpx;
    line-height:110rpx;
    border-bottom:1px solid #f9f;
    padding:0 20rpx;
}
.time{
   padding:0 20rpx;
   line-height:90rpx; 
}
.add{
    position:absolute;
    bottom:80rpx;
    right:80rpx;
    border:1px solid #000;
    width:100rpx;
    height:100rpx;
    border-radius: 50%;
    display:flex;
    align-items: center;
    justify-content: center;
    background: #9ff;
}
.add image{
    width:50rpx;
    height:50rpx;
}